# - Config file for the apriltags package
# It defines the following variables
#  apriltagscpp_INCLUDE_DIRS - include directories for apriltagscpp
#  apriltagscpp_LIBRARIES    - libraries to link against
#  apriltagscpp_EXECUTABLE   - the maketags executable
 
# Compute paths
get_filename_component(apriltagscpp_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(apriltagscpp_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")
 
# Our library dependencies (contains definitions for IMPORTED targets)
if(NOT TARGET apriltags AND NOT ${PROJECT_NAME}_BINARY_DIR)
  include("${apriltagscpp_CMAKE_DIR}/${PROJECT_NAME}Targets.cmake")
endif()
 
# These are IMPORTED targets created by ${PROJECT_NAME}Targets.cmake
set(apriltagscpp_LIBRARIES apriltagscpp)
set(apriltagscpp_EXECUTABLE maketags)